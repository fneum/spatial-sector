@article{Naegler_2015,
	title = {Quantification of the {European} industrial heat demand by branch and temperature level},
	volume = {39},
	copyright = {Copyright © 2015 John Wiley \& Sons, Ltd.},
	issn = {1099-114X},
	url = {https://onlinelibrary.wiley.com/doi/abs/10.1002/er.3436},
	doi = {10.1002/er.3436},
	abstract = {We present the first comprehensive estimate of the final energy demand for heat in all EU28 member states for the reference year 2012, differentiated by temperature levels, comparing two different approaches. Two different calculation approaches based on different data sets yielded estimates of the total final energy demand for heat in the EU28 of 8150 PJ and 8518 PJ in 2012, respectively. Approach 1 distinguishes between three different process heat (PH) temperature levels and results in final energy demand for heat {\textless}100°C: 2077 PJ, 100–400°C: 2214 PJ and {\textgreater}400°C: 3859 PJ. The second approach distinguishes between low temperature space heat and hot water ({\textless}100°C: 1161 PJ) and four different PH temperature levels with a resulting energy demand of {\textless}100°C: 1027 PJ, 100–500°C: 1785 PJ, 500–1000°C: 1679 PJ and {\textgreater}1000°C: 2865 PJ. The high share of high-temperature heat illustrates the limits to the potential decarbonisation of industrial thermal processes with renewable energy sources such as (non-concentrating) solar thermal, geothermal or environmental heat. Therefore specific information on required temperature levels is of the essence. This, in turn, points out the relevance of renewable electricity and synthetic fuels based on renewable power for a significant reduction of CO2 emissions from the industry sector in Europe. Considering current data quality, it is recommended to develop a consistent, comprehensive methodology to significantly improve the data basis on industrial heat demand. Copyright © 2015 John Wiley \& Sons, Ltd.},
	language = {en},
	number = {15},
	urldate = {2018-12-10},
	journal = {International Journal of Energy Research},
	author = {Naegler, Tobias and Simon, Sonja and Klein, Martin and Gils, Hans Christian},
	month = dec,
	year = {2015},
	keywords = {high temperature heat, industrial heat demand, low temperature heat, process heat},
	pages = {2019--2030}
}


